{
	"image": "ubuntu:22.04",
  "customizations": {
    "vscode": {
	    "extensions": [
        "ms-dotnettools.csharp",
        "icsharpcode.ilspy-vscode",
        "denoland.vscode-deno",
        "appulate.filewatcher"
      ]
	  }
	},
  "features": {
    "ghcr.io/dragos-tudor/devcontainer-features/debian-packages:1.2": {},
    "ghcr.io/dragos-tudor/devcontainer-features/debian-user-group:1.3": {},
    "ghcr.io/dragos-tudor/devcontainer-features/debian-dotnet-product:1.2": {},
    "ghcr.io/dragos-tudor/devcontainer-features/debian-deno:1.5": {}
  },
  "containerEnv": { "DATA_PROTECTION_KEY": "HFm8fYbLyEarjjlGBj5r9Q==" },
  "mounts": [
		"source=nuget,target=/home/vscode/.nuget",
		"source=deno-cache,target=/home/vscode/.deno-cache"
  ],
	"runArgs": [
		"--name=backend-security",
		"--hostname=security"
	],
  "remoteUser": "vscode"
}
